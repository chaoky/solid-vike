{
        "compilerOptions": {
                "strict": true,
                "module": "ES2020",
                "moduleResolution": "Node",
                "target": "ES2020",
                "lib": [
                        "DOM",
                        "DOM.Iterable",
                        "ESNext"
                ],
                "types": [
                        "vite/client",
                        "bun-types"
                ],
                "jsx": "preserve",
                "jsxImportSource": "solid-js",
                "skipLibCheck": true,
                "esModuleInterop": true,
                "noEmit": true,
                "paths": {
                        "/*": [
                                "./*"
                        ],
                }
        }
}
